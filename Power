#include<bits/stdc++.h>
using namespace std;

int power(int n){
    if(n==0)return 1;
    
    return 2*power(n-1);
}

int main(){
    int  n;
    cin>>n;
    int ans=power(n);
    cout<<ans<<endl;
}


#include <iostream>

using namespace std;

int pow(int x,int n){
    
    if(n==0)return 1;
    
    return x*pow(x,n-1);
}

int main()
{
  int n=2,x=10;
  
  int ans=pow(10,2);
cout<<ans<<endl;
    return 0;
}
